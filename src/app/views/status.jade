doctype html(lang="de")

html
  head
    title Status
    script(src="/js/jquery-1.11.1.js")

  body
    h1 Status

    div
      each room, index in allstate.room
        div
          h1 Raum #{room}
          ul
          each person, i in room
            li
              span( class='#{person.line}' ) #{person.line}
              = ' '
              span#info #{person.name} [-#{person.number}]


